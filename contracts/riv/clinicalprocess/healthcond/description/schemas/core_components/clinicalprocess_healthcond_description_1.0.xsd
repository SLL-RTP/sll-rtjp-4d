<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema' xmlns:tns='urn:riv:clinicalprocess:healthcond:description:1' targetNamespace='urn:riv:clinicalprocess:healthcond:description:1' elementFormDefault='qualified' attributeFormDefault='unqualified' version='1.0'>
  <xs:complexType name='SomeElementType'>
    <xs:sequence>
      <xs:element name='SomeElement' type='xs:string' />
      <xs:any namespace='##other' processContents='lax' minOccurs='0' maxOccurs='unbounded' />
    </xs:sequence>
  </xs:complexType>
 
  <!-- @TODO Should we have maxlength 64? -->
  <xs:simpleType name="HSAIdType">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
 
  <xs:simpleType name="PatientIdType">
    <xs:annotation>
      <xs:documentation>
        Patient identification is represented by the swedish personal identity number (se: personnummer). 
        yyyymmddnnnn
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{8}[0-9pPtTfF][0-9]{3}"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="PatientType">
    <xs:sequence>
      <xs:element name="patientId" type="tns:PatientIdType"/>
      <xs:element name="givenName" type="xs:string" minOccurs="0"/>
       <xs:element name="surname" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="PersonalIdentityNumberType">
    <xs:annotation>
      <xs:documentation>
        Swedish Personal Identity Number (se: personnummer). 
        yyyymmddnnnn
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{8}[0-9pPtTfF][0-9]{3}"/>
    </xs:restriction>
  </xs:simpleType> 
</xs:schema>
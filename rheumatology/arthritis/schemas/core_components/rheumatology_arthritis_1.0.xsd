<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema' xmlns:tns='urn:riv:rheumatology:arthritis:1' targetNamespace='urn:riv:rheumatology:arthritis:1' elementFormDefault='qualified' attributeFormDefault='unqualified' version='1.0'>

  
  <!-- 
        Below: only helper types allowed.
        
        A helper type is a type used in one ore more of the domain schema common types.
        It can also be a type used in two or more service schemas.
    -->
   
  <xs:simpleType name="PatientIdType">
    <xs:annotation>
      <xs:documentation>
        personnummer: yyyymmddnnnn
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{8}[0-9pPtTfF][0-9]{3}"/>
    </xs:restriction>
  </xs:simpleType>
  
  
</xs:schema>